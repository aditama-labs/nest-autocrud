{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@autocrud/skeleton/(.*)": "<rootDir>/../libs/skeleton/src/$1",
    "@autocrud/skeleton": "<rootDir>/../libs/skeleton/src"
  }
}